#include <iostream>

using namespace std;

int main()
{
    string number;
    cin >> number;
    
    int i = 0;
    
    for (int i = 0; i < number.size(); i++) {
        int x = number[i] - '0';
        cout << x << ": ";
        
        for (int j = 0; j < x; j++) {
            cout << x;
        }
        
        cout << endl;
    }

    return 0;
}
